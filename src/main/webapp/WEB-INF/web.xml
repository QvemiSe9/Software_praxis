  	<servlet-class>com.servlet.BuyServlet</servlet-class>
  </servlet>

  <servlet-mapping>
  	<servlet-name>Buyer_message_servlet</servlet-name>
  	<url-pattern>/Buyer_message_servlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>password_change_servlet</servlet-name>
  	<url-pattern>/password_change_servlet</url-pattern>
